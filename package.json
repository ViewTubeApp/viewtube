    "db:reset": "SKIP_ENV_VALIDATION=true SKIP_SEED=true tsx drizzle.seed.ts",
    "dev": "next dev --turbo",
    "analyze": "ANALYZE=true next build",
    "debug": "NODE_OPTIONS='--inspect' next dev --turbo",
    "format:ts": "prettier --write \"**/*.{ts,tsx,js,jsx,mdx}\" --cache",
    "format:go": "cd extra/hermes && go fmt ./...",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "preview": "next build && next start",
    "start": "next start",
    "type-check": "tsc --noEmit",
    "intl:validate": "./scripts/validate-keys.sh",
    "intl:compile": "bunx @inlang/paraglide-js compile --project ./project.inlang --outdir ./src/paraglide",
    "check-updates": "bunx npm-check-updates -i"
